<script src="/js/dygraph.js"></script>
<script src="/js/synchronizer.js"></script>
<script src="/js/crosshair.js"></script>
<script src="/js/utils.js"></script>
<link rel="stylesheet" type="text/css" href="/css/dygraph.css">

</head>

<body>

    <%- include('./includes/nav.ejs') %>
    <main>
        <div class="container">
            <h1 class="font-weight-light text-center"><%= csv %></h1>
            <div id="chart" style="width: 100%;"></div>
        </div>
    </main>
    <script>
        g = new Dygraph(document.getElementById("chart"), '/data/long-term/<%= csv %>', 
                {
                    // options go here. See http://dygraphs.com/options.html
                    legend: 'always',
                    animatedZooms: false,
                    drawGrid: false,
                    plugins: [
                        new Dygraph.Plugins.Crosshair({
                        direction: "both"
                        })
                    ]
                });
    </script>
<%- include('./includes/end.ejs') %>